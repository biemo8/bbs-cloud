<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biemo.cloud.system.modular.ent.mapper.EntUserAccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.biemo.cloud.system.modular.ent.entity.EntUserAccount">
        <id column="account_id" property="accountId" />
        <result column="account" property="account" />
        <result column="password" property="password" />
        <result column="salt" property="salt" />
        <result column="user_id" property="userId" />
        <result column="company_id" property="companyId" />
        <result column="create_time" property="createTime" />
        <result column="create_user" property="createUser" />
        <result column="update_time" property="updateTime" />
        <result column="update_user" property="updateUser" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        account_id AS "accountId", account AS "account", password AS "password", salt AS "salt", user_id AS "userId", company_id AS "companyId", create_time AS "createTime", create_user AS "createUser", update_time AS "updateTime", update_user AS "updateUser", del_flag AS "delFlag"
    </sql>


    <select id="customList" resultType="com.biemo.cloud.system.modular.ent.model.result.EntUserAccountResult" parameterType="com.biemo.cloud.system.modular.ent.model.params.EntUserAccountParam">
        select
        <include refid="Base_Column_List"/>
        from biemo_ent_user_account where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="com.biemo.cloud.system.modular.ent.model.params.EntUserAccountParam">
        select
        <include refid="Base_Column_List"/>
        from biemo_ent_user_account where 1 = 1
    </select>

    <select id="customPageList" resultType="com.biemo.cloud.system.modular.ent.model.result.EntUserAccountResult" parameterType="com.biemo.cloud.system.modular.ent.model.params.EntUserAccountParam">
        select
        <include refid="Base_Column_List"/>
        from biemo_ent_user_account where 1 = 1
    </select>

    <select id="customPageMapList" resultType="map" parameterType="com.biemo.cloud.system.modular.ent.model.params.EntUserAccountParam">
        select
        <include refid="Base_Column_List"/>
        from biemo_ent_user_account where 1 = 1
    </select>

</mapper>
